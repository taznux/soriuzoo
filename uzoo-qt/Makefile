####### Compiler, tools and options

CC	=	gcc
CXX	=	g++
CFLAGS	=	-pipe -Wall -W -O2 -DNO_DEBUG
CXXFLAGS=	-pipe -Wall -W -O2 -DNO_DEBUG
INCPATH	=	-I$(QTDIR)/include
LINK	=	g++
LFLAGS	=	
LIBS	=	$(SUBLIBS) -L$(QTDIR)/lib -L/usr/X11R6/lib -lqt -lXext -lX11 -lm
MOC	=	$(QTDIR)/bin/moc
UIC	=	$(QTDIR)/bin/uic

TAR	=	tar -cf
GZIP	=	gzip -9f

####### Files

HEADERS =	http.h \
		uzoomain.h \
		uzoomainnet.h \
		uzooparser.h \
		uzoosocket.h
SOURCES =	http.cpp \
		main.cpp \
		uzoomain.cpp \
		uzoomainnet.cpp \
		uzooparser.cpp \
		uzoosocket.cpp
OBJECTS =	http.o \
		main.o \
		uzoomain.o \
		uzoomainnet.o \
		uzooparser.o \
		uzoosocket.o
INTERFACES =	
UICDECLS =	
UICIMPLS =	
SRCMOC	=	moc_http.cpp \
		moc_uzoomain.cpp \
		moc_uzoomainnet.cpp \
		moc_uzooparser.cpp \
		moc_uzoosocket.cpp
OBJMOC	=	moc_http.o \
		moc_uzoomain.o \
		moc_uzoomainnet.o \
		moc_uzooparser.o \
		moc_uzoosocket.o
DIST	=	
TARGET	=	uzoo
INTERFACE_DECL_PATH = .

####### Implicit rules

.SUFFIXES: .cpp .cxx .cc .C .c

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules


all: $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

moc: $(SRCMOC)

tmake: Makefile

Makefile: uzoo.pro
	tmake uzoo.pro -o Makefile

dist:
	$(TAR) uzoo.tar uzoo.pro $(SOURCES) $(HEADERS) $(INTERFACES) $(DIST)
	$(GZIP) uzoo.tar

clean:
	-rm -f $(OBJECTS) $(OBJMOC) $(SRCMOC) $(UICIMPLS) $(UICDECLS) $(TARGET)
	-rm -f *~ core

