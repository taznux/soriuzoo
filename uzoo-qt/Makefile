###### Combined headers


####### Compile

http.o: http.cpp \
		http.h

main.o: main.cpp \
		http.h \
		uzoomainnet.h \
		uzoomain.h \
		uzooparser.h

uzoomain.o: uzoomain.cpp \
		uzoomain.h

uzoomainnet.o: uzoomainnet.cpp \
		uzoomainnet.h \
		uzoomain.h \
		uzooparser.h \
		uzoosocket.h

uzooparser.o: uzooparser.cpp \
		uzooparser.h

uzoosocket.o: uzoosocket.cpp \
		uzoosocket.h

moc_http.o: moc_http.cpp \
		http.h

moc_uzoomain.o: moc_uzoomain.cpp \
		uzoomain.h

moc_uzoomainnet.o: moc_uzoomainnet.cpp \
		uzoomainnet.h \
		uzoomain.h \
		uzooparser.h

moc_uzooparser.o: moc_uzooparser.cpp \
		uzooparser.h

moc_uzoosocket.o: moc_uzoosocket.cpp \
		uzoosocket.h

moc_http.cpp: http.h
	$(MOC) http.h -o moc_http.cpp

moc_uzoomain.cpp: uzoomain.h
	$(MOC) uzoomain.h -o moc_uzoomain.cpp

moc_uzoomainnet.cpp: uzoomainnet.h
	$(MOC) uzoomainnet.h -o moc_uzoomainnet.cpp

moc_uzooparser.cpp: uzooparser.h
	$(MOC) uzooparser.h -o moc_uzooparser.cpp

moc_uzoosocket.cpp: uzoosocket.h
	$(MOC) uzoosocket.h -o moc_uzoosocket.cpp

